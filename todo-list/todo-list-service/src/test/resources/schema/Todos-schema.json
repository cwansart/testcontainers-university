{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "todos": {
    "type": "object",
    "title": "List Todo",
    "description": "A reduced todo with identifier used as response object for the todo list.",
    "properties": {
      "id": {
        "type": "integer"
      },
      "title": {
        "type": "string"
      },
      "dueDate": {
        "type": "string",
        "format": "date-time"
      },
      "done": {
        "type": "boolean"
      }
    },
    "required": [
      "id",
      "title",
      "dueDate",
      "done"
    ]
  }
}