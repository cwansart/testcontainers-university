{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "properties": {
    "errors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "example": "UNKNOWN_ERROR"
          },
          "message": {
            "type": "string",
            "example": "An unknown error occurred"
          }
        }
      }
    },
    "timestamp": {
      "format": "date-time",
      "type": "string",
      "example": "2019-07-01T12:34:56.789+02:00"
    },
    "uuid": {
      "type": "string",
      "example": "55e6e986-767b-433a-9544-ddc8e25d67ab"
    }
  }
}